  /* 
  * This is a launch config file for VS Code that will attach to a chrome browser or launch the chrome browser
  * itself to make code debuggable via VS Code (could also be done via sourcemap and browser but without the VS Code features).
  */
  "version": "0.2.0",
  "configurations": [
      {
        "name": "Launch interfaces-classes (edge)",
        "request": "launch",
        "type": "msedge",
        "url": "http://localhost:3000",
        "webRoot": "${workspaceFolder}/interfaces-classes"
      },
      {
        "name": "Launch advanced types (edge)",
        "request": "launch",
        "type": "msedge",
        "url": "http://localhost:3000",
        "webRoot": "${workspaceFolder}/adv-types"
      },
      {
        "name": "Launch typescript-nextgen",
        "request": "launch",
        "type": "chrome",
        "url": "http://localhost:3000",
        "webRoot": "${workspaceFolder}/typescript-nextgen"
      },
      {
        "name": "Launch interfaces-classes (chrome)",
        "request": "launch",
        "type": "chrome",
        "url": "http://localhost:3000",
        "webRoot": "${workspaceFolder}/interfaces-classes"
    }
  ]
}